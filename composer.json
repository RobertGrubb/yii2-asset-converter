{
    "name": "robertgrubb/yii2-asset-converter",
    "description": "Less, Sass, Scss and Phamlp converter for Yii2. No system requires. yii2-composer support, Less autoupdate, customizing of output directory",
    "type": "yii2-extension",
    "keywords": ["yii", "yii2", "asset", "converter", "less", "sass", "scss", "compass", "phamlp"],
    "license": "BSD-3-Clause",
    "version": "1.3.1",
    "authors": [
        {
            "name": "Alex Sharov",
            "email": "www.pismeco@gmail.com",
            "role": "Author"
        },
        {
            "name": "Robert Grubb",
            "email": "mgrubb0507@gmail.com",
            "role": "Contributor"
        }
    ],
    "require" : {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.*",
        "yiisoft/yii2-composer": "2.*",
        "oyejorge/less.php": "1.7.0.*",
        "RobertGrubb/phpsass": "1.*",
        "leafo/scssphp": "0.*",
        "yourilima/scssphp-compass": "1.*"
    },
    "autoload": {
        "psr-0": { "robertgrubb\\assetConverter\\": "" }
    },
    "target-dir": "robertgrubb/assetConverter"
}
